{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "linebreak-style":"off",
        "no-multi-spaces":"off",
        "no-tabs":"off",
        "indent": ["off", "tab"],
        "quotes":["error", "double"],
        "max-len":"warn",
        "import/order":"off"
    }
}
